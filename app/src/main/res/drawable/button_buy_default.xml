<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <item android:right="2dp" android:left="2dp" android:bottom="4dp">
        <shape >
            <corners
                android:radius="5dp"
                />
            <solid
                android:color="#20FFFFFF"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />

            <stroke
                android:width="2dp"
                android:color="#FFC724"
                />

        </shape>
    </item>



</layer-list>

