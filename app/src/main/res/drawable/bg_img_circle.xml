<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            >
            <size
                android:width="70dp"
                android:height="70dp"
                />

            <stroke
                android:width="10dp"
                android:color="@android:color/white" />
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle"
            >

            <size
                android:width="60dp"
                android:height="60dp"
                />

            <stroke
                android:width="10dp"
                android:color="@android:color/white"
                />

        </shape>


    </item>

</layer-list>