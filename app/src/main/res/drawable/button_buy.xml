<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <item>
        <shape >
            <corners
                android:radius="5dp"
                />
            <solid
                android:color="#20FFFFFF"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />

            <stroke
                android:width="2dp"
                android:color="#FFC724"
                />

        </shape>
    </item>
    <item android:drawable="@drawable/shopping_cart" android:start="60dp" android:top="3dp" android:end="10dp" android:bottom="3dp" android:height="30dp" android:width="30dp"/>


</layer-list>

